<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <bootstrap3-test-templates-set j:dependencies="bootstrap3-components bootstrap3-core default facets search"
                                     j:installedModules="bootstrap3-test-templates-set"
                                     j:modulePriority="0"
                                     j:moduleType="templatesSet"
                                     j:title="bootstrap3 Test Templates Set"
                                     jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                     jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder">
                        <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder"/>
                        </themes>
                    </files>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:theme="/modules/bootstrap3-test-templates-set/templates/files/themes/default"
                  j:view="bootstrap3-test-templates-set"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
                        <home jcr:primaryType="jnt:pageTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <section--container--row--grid columnsType="nogrid"
                                                    gridLayout="fixed-width"
                                                    jcr:mixinTypes="bootstrap3mix:createAbsoluteAreas bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection"
                                                    jcr:primaryType="bootstrap3nt:columns"
                                                    level="0"
                                                    sectionElement="div"/>
                                <section--container--row--grid-1 columnsType="customColumns"
                                                      gridClasses="col-md-12"
                                                      gridLayout="fixed-width"
                                                      jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection bootstrap3mix:customColumns"
                                                      jcr:primaryType="bootstrap3nt:columns"
                                                      sectionElement="div"/>
                            </pagecontent>
                        </home>
                        <simple jcr:primaryType="jnt:pageTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <section--container--row--grid columnsType="nogrid"
                                                    gridLayout="fixed-width"
                                                    jcr:mixinTypes="bootstrap3mix:createAbsoluteAreas bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection"
                                                    jcr:primaryType="bootstrap3nt:columns"
                                                    level="0"
                                                    sectionElement="div"/>
                                <simple columnsType="predefinedColumns"
                             grid="12"
                             gridLayout="fixed-width"
                             jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection bootstrap3mix:predefinedColumns"
                             jcr:primaryType="bootstrap3nt:columns"
                             sectionElement="div"/>
                            </pagecontent>
                        </simple>
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <section--container--row--grid columnsType="predefinedColumns"
                                                 containerCssClass="classContainer"
                                                 containerId="IdContainer"
                                                 grid="12"
                                                 gridLayout="fixed-width"
                                                 jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection bootstrap3mix:predefinedColumns"
                                                 jcr:primaryType="bootstrap3nt:columns"
                                                 rowCssClass="classRow"
                                                 rowId="idRow"
                                                 sectionCssClass="classHeader"
                                                 sectionElement="header"
                                                 sectionId="idHeader"
                                                 sectionRole="role-header">
                                <section--container--row--grid-side jcr:primaryType="jnt:contentList"/>
                                <section--container--row--grid-main jcr:primaryType="jnt:contentList">
                                    <enhanced-navbar fluid="true"
                                         inverse="false"
                                         jcr:primaryType="bootstrap3nt:enhancedNavBar"
                                         option="">
                                        <navigation-menu j:baselineNode="home"
                                            j:maxDepth="5"
                                            j:startLevel="0"
                                            jcr:primaryType="bootstrap3nt:navBarItemMenu"
                                            position="right"/>
                                        <search-form jcr:primaryType="bootstrap3nt:navBarItemSimpleSearchForm"
                                        position="right"
                                        result="/modules/bootstrap3-test-templates-set/search-result"/>
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Home"/>
                                        <login j:view="tst-loginView"
                                  jcr:mixinTypes="jmix:renderable"
                                  jcr:primaryType="bootstrap3nt:navBarItemLoginForm"
                                  position="right"
                                  view="menu"/>
                                        <language-switcher jcr:primaryType="bootstrap3nt:navBarItemLanguageSwitcher"
                                              position="right"
                                              typeOfDisplay="isoLocaleCountryCode"/>
                                    </enhanced-navbar>
                                </section--container--row--grid-main>
                            </section--container--row--grid>
                            <pagecontent jcr:primaryType="jnt:area"/>
                            <container fluid="true" jcr:primaryType="bootstrap3nt:container">
                                <container jcr:primaryType="jnt:contentList">
                                    <row jcr:primaryType="bootstrap3nt:row">
                                        <column jcr:primaryType="bootstrap3nt:column" offset="0" span="12">
                                            <column jcr:primaryType="jnt:contentList">
                                                <version jcr:primaryType="bootstrap3nt:version"/>
                                            </column>
                                        </column>
                                    </row>
                                </container>
                            </container>
                        </pagecontent>
                        <testtmplb jcr:primaryType="jnt:pageTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <section--container--row--grid columnsType="nogrid"
                                                    gridLayout="fixed-width"
                                                    jcr:mixinTypes="bootstrap3mix:createAbsoluteAreas bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection"
                                                    jcr:primaryType="bootstrap3nt:columns"
                                                    level="0"
                                                    sectionElement="div"/>
                                <simple columnsType="predefinedColumns"
                             grid="12"
                             gridLayout="fixed-width"
                             jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection bootstrap3mix:predefinedColumns"
                             jcr:primaryType="bootstrap3nt:columns"
                             sectionElement="div">
                                    <simple-main jcr:primaryType="jnt:contentList">
                                        <template-b jcr:primaryType="jnt:text">
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                text="template-b"/>
                                            <j:translation_de jcr:language="de"
                                                jcr:primaryType="jnt:translation"
                                                text="template-b"/>
                                            <j:translation_pt jcr:language="pt"
                                                jcr:primaryType="jnt:translation"
                                                text="template-b"/>
                                            <j:translation_it jcr:language="it"
                                                jcr:primaryType="jnt:translation"
                                                text="template-b"/>
                                            <j:translation_fr jcr:language="fr"
                                                jcr:primaryType="jnt:translation"
                                                text="template-b"/>
                                            <j:translation_es jcr:language="es"
                                                jcr:primaryType="jnt:translation"
                                                text="template-b"/>
                                        </template-b>
                                    </simple-main>
                                </simple>
                            </pagecontent>
                            <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmplb"/>
                            <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmplb"/>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmplb"/>
                            <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmplb"/>
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmplb"/>
                            <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmplb"/>
                        </testtmplb>
                        <testtmpla jcr:primaryType="jnt:pageTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <section--container--row--grid columnsType="nogrid"
                                                    gridLayout="fixed-width"
                                                    jcr:mixinTypes="bootstrap3mix:createAbsoluteAreas bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection"
                                                    jcr:primaryType="bootstrap3nt:columns"
                                                    level="0"
                                                    sectionElement="div"/>
                                <simple columnsType="predefinedColumns"
                             grid="12"
                             gridLayout="fixed-width"
                             jcr:mixinTypes="bootstrap3mix:createContainer bootstrap3mix:createRow bootstrap3mix:createSection bootstrap3mix:predefinedColumns"
                             jcr:primaryType="bootstrap3nt:columns"
                             sectionElement="div">
                                    <simple-main jcr:primaryType="jnt:contentList">
                                        <template-a jcr:primaryType="jnt:text">
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                text="template-a"/>
                                            <j:translation_de jcr:language="de"
                                                jcr:primaryType="jnt:translation"
                                                text="template-a"/>
                                            <j:translation_pt jcr:language="pt"
                                                jcr:primaryType="jnt:translation"
                                                text="template-a"/>
                                            <j:translation_it jcr:language="it"
                                                jcr:primaryType="jnt:translation"
                                                text="template-a"/>
                                            <j:translation_fr jcr:language="fr"
                                                jcr:primaryType="jnt:translation"
                                                text="template-a"/>
                                            <j:translation_es jcr:language="es"
                                                jcr:primaryType="jnt:translation"
                                                text="template-a"/>
                                        </template-a>
                                    </simple-main>
                                </simple>
                            </pagecontent>
                            <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmpla"/>
                            <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmpla"/>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmpla"/>
                            <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmpla"/>
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmpla"/>
                            <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="testtmpla"/>
                        </testtmpla>
                    </base>
                </templates>
                
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                </home>
                <search-result j:templateName="simple" jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Search result"/>
                    <j:translation_de jcr:language="de"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Search result"/>
                    <j:translation_pt jcr:language="pt"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Search result"/>
                    <j:translation_it jcr:language="it"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Search result"/>
                    <j:translation_fr jcr:language="fr"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Search result"/>
                    <j:translation_es jcr:language="es"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Search result"/>
                    <j:translation_de_DE jcr:language="de_DE"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Search result"/>
                    <simple-main jcr:primaryType="jnt:contentList">
                        <advanced-search jcr:primaryType="jnt:advancedSearchForm">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Advanced search"/>
                            <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Advanced search"/>
                            <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Advanced search"/>
                            <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Advanced search"/>
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Advanced search"/>
                            <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Advanced search"/>
                            <j:translation_de_DE jcr:language="de_DE"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Advanced search"/>
                        </advanced-search>
                        <search-results autoSuggest="true"
                               autoSuggestHitCount="2"
                               autoSuggestMaxTermCount="1"
                               autoSuggestMinimumHitCount="2"
                               jcr:primaryType="jnt:searchResults">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Search results"/>
                            <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Search results"/>
                            <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Search results"/>
                            <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Search results"/>
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Search results"/>
                            <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Search results"/>
                            <j:translation_de_DE jcr:language="de_DE"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Search results"/>
                        </search-results>
                    </simple-main>
                </search-result>
            </bootstrap3-test-templates-set>
    </modules>
</content>